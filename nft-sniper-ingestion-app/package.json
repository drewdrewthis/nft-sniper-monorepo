  "scripts": {
    "prebuild": "rimraf dist",
    "migrate:postgres": "yarn dotenv -e .env.development yarn prisma migrate dev",
    "build": "nest build",
    "build:image": "docker buildx build --platform=linux/amd64 . -t drewdrewthis/alpha-sniper:ingestion-server && docker push drewdrewthis/alpha-sniper:ingestion-server",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "yarn dotenv -e .env.development nest start --watch",
    "dev": "yarn dotenv -e .env.development",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
