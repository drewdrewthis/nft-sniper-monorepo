  "scripts": {
    "test:env": "yarn dotenv -e .env.test",
    "dev:env": "yarn dotenv -e .env.development",
    "postgres:migrate": "yarn prisma migrate dev",
    "postgres:seed": "yarn prisma db seed",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:image": "docker buildx build --platform=linux/amd64 . -t drewdrewthis/alpha-sniper:ingestion-server && docker push drewdrewthis/alpha-sniper:ingestion-server",
    "prep_for_deploy": "yarn lint && yarn test && yarn build:image",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:migrate:postgres": "yarn test:env yarn prisma migrate dev",
    "test:e2e": "yarn jest --config ./test/jest-e2e.ts"
  },
