# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings

POSTGRES_USER=admin
POSTGRES_DB=ingestion-server
REDIS_HOST=localhost
POSTGRES_HOST=localhost
DATABASE_URL=postgresql://$POSTGRES_USER:$POSTGRES_PASSWORD@$POSTGRES_HOST:5433/ingestion-server?schema=public
REDIS_PORT=6380

# Crawler
X2Y2_SCHEDULER_FREQUENCY_MS=0
OPENSEA_SCHEDULER_FREQUENCY_MS=0

# RESEVOIR
RESEVOIR_RATE_LIMIT_MAX_RPS=4

# ENDPOINTS
CRAWLER_SERVER_CRAWL_ENDPOINT=http://localhost:3001/crawl

# SECRETS
JWT_SECRET_KEY=development-jwt-secret-key
POSTGRES_PASSWORD=password
X2Y2_API_KEY=bogus-api-key
RESEVOIR_API_KEY=bogus-api-key
